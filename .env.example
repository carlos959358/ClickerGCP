# Environment Variables Example
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control - it contains sensitive data

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-southwest1

# Cloud Run Services
BACKEND_SERVICE_NAME=clicker-backend
CONSUMER_SERVICE_NAME=clicker-consumer
BACKEND_URL=https://your-backend-url.run.app
CONSUMER_URL=https://your-consumer-url.run.app

# Artifact Registry
ARTIFACT_REPO=clicker-repo

# Pub/Sub
PUBSUB_TOPIC=click-events
PUBSUB_SUBSCRIPTION=click-consumer-sub

# Firestore
FIRESTORE_DATABASE=clicker-db

# Docker Registry
DOCKER_REGISTRY_REGION=europe-southwest1

# Deployment
DEPLOY_REGION=europe-southwest1
